# Core model providers
OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
AZURE_OPENAI_API_KEY=
GROQ_API_KEY=

# IBM Watsonx / Granite 4.0
WATSONX_API_KEY=                 # IBM Cloud API key (required for watsonx)
WATSONX_PROJECT_ID=              # Watsonx project ID (required for watsonx)
WATSONX_URL=                     # Watsonx API endpoint (optional, defaults to IBM Cloud)
# Available Granite 4.0 models: granite-4-h-small, granite-4-h-micro, granite-4-h-tiny

# Memory + RAG
VECTOR_BACKEND=chroma  # chroma|qdrant|weaviate|milvus|local
VECTOR_HOST=localhost
VECTOR_PORT=6333
VECTOR_NAMESPACE=cuga-demo
RAG_ENABLED=false

# Observability
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_HOST=
OPENINFERENCE_ENABLED=false
TRACELOOP_API_KEY=

# Sandbox + registry
PROFILE=demo_power
REGISTRY_FILE=registry.yaml
MCP_SERVERS_FILE=./build/mcp_servers.demo_power.yaml

# Telemetry
TELEMETRY_OPT_IN=false

# Web/CLI
PORT=8000
LOG_LEVEL=INFO
